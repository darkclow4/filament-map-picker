{
    "name": "darkclow4/filament-map-picker",
    "description": "Package for get lat lang using leaflet",
    "keywords": [
        "darkclow4",
        "laravel",
        "filamentphp",
        "filament-map-picker"
    ],
    "homepage": "https://github.com/darkclow4/filament-map-picker",
    "support": {
        "issues": "https://github.com/darkclow4/filament-map-picker/issues",
        "source": "https://github.com/darkclow4/filament-map-picker"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "darkclow4",
            "email": "irfan.140895@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/filament": "^4.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "larastan/larastan": "^3.0",
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^8.0",
        "orchestra/testbench": "^9.0|^10.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-arch": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "pestphp/pest-plugin-livewire": "^3.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Darkclow4\\MapPicker\\": "src/",
            "Darkclow4\\MapPicker\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Darkclow4\\MapPicker\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "phpstan analyse",
        "lint": "pint",
        "test": "pest",
        "test:lint": "pint --test"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Darkclow4\\MapPicker\\MapPickerServiceProvider"
            ],
            "aliases": {
                "MapPicker": "Darkclow4\\MapPicker\\Facades\\MapPicker"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}